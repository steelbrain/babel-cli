{
  "name": "sb-babel-cli",
  "bin": {
    "sb-babel-cli": "./lib/cli/index.js"
  },
  "files": [
    "lib/*"
  ],
  "author": "steelbrain",
  "readme": "./README.md",
  "version": "4.0.0-beta1",
  "description": "A smarter babel-cli",
  "repository": "https://github.com/steelbrain/babel-cli",
  "license": "MIT",
  "keywords": [
    "cli",
    "babel"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "prepare": "yarn build",
    "build": "tsc --module commonjs --target es5 --outDir lib",
    "watch": "yarn build -w",
    "lint": "(tsc -p . --noEmit) && (eslint . --ext .ts) && (prettier --list-different src/**/*.ts)"
  },
  "dependencies": {
    "anymatch": "^3.1.1",
    "chalk": "^4.1.0",
    "chokidar": "^3.4.3",
    "commander": "^6.1.0",
    "del": "^6.0.0",
    "lodash": "^4.17.15",
    "lowdb": "^1.0.0",
    "make-dir": "^3.0.0",
    "p-map": "^4.0.0",
    "resolve-from": "^5.0.0",
    "sb-promise-queue": "^2.1.0"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/babel__core": "^7.1.10",
    "@types/lodash": "^4.14.162",
    "@types/lowdb": "^1.0.9",
    "@types/node": "^14.11.10",
    "eslint-config-steelbrain": "11.0.0",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">= 10"
  }
}
