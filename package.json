{
  "name": "sb-babel-cli",
  "main": "lib/index.js",
  "bin": {
    "sb-babel-cli": "./lib/cli/index.js"
  },
  "author": "steelbrain",
  "readme": "./README.md",
  "version": "2.0.0",
  "description": "A smarter babel-cli",
