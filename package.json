{
  "name": "sb-babel-cli",
  "main": "lib/index.js",
  "bin": {
    "sb-babel-cli": "./lib/cli/index.js"
  },
  "files": [
    "lib/*"
  ],
  "author": "steelbrain",
  "readme": "./README.md",
  "version": "3.1.2",
  "description": "A smarter babel-cli",
  "repository": "https://github.com/steelbrain/babel-cli",
  "license": "MIT",
  "keywords": [
    "cli",
    "babel"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "prepare": "yarn build",
    "build": "yarn clean && babel src --out-dir lib",
    "watch": "yarn build -- -w",
    "lint": "(eslint . ) && (prettier --list-different src/*.js)"
  },
  "dependencies": {
    "anymatch": "^3.1.1",
    "chalk": "^4.1.0",
    "chokidar": "^3.3.1",
    "commander": "^6.1.0",
    "del": "^6.0.0",
    "lodash": "^4.17.15",
    "lowdb": "^1.0.0",
    "make-dir": "^3.0.0",
    "p-map": "^4.0.0",
    "p-queue": "^6.2.1",
    "pify": "^5.0.0",
    "resolve-from": "^5.0.0",
    "sb-fs": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "eslint-config-steelbrain": "10.0.0-beta2",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">= 10"
  }
}
